@import '../../res/style/common.wxss';
/**index.wxss**/

#container-index {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}

#container-index .tip{
    text-align: center;
    margin-bottom: 100rpx;
}

.icon-loading {
    width:88rpx;
    height:88rpx;
    animation: rotate-360 1s linear infinite;
}


@keyframes rotate-360 {
    0% {
        transform: rotate(0deg);
    }
    100% {
        transform: rotate(360deg);
    }
  }